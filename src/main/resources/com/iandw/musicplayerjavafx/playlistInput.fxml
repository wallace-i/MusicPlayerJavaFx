<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="98.0" prefWidth="266.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iandw.musicplayerjavafx.PlaylistInput">
   <children>
      <FlowPane prefHeight="48.0" prefWidth="266.0">
         <children>
            <Label prefHeight="27.0" prefWidth="80.0" text="New Playlist:">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <FlowPane.margin>
                  <Insets top="10.0" />
               </FlowPane.margin>
            </Label>
            <TextField fx:id="playlistNameTextInput" prefHeight="25.0" prefWidth="174.0">
               <padding>
                  <Insets bottom="5.0" left="50.0" right="5.0" top="5.0" />
               </padding>
               <FlowPane.margin>
                  <Insets top="10.0" />
               </FlowPane.margin>
            </TextField>
         </children>
      </FlowPane>
      <Button fx:id="okButton" onMouseClicked="okButtonClicked" layoutX="59.0" layoutY="59.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" scaleShape="false" text="OK" />
      <Button fx:id="cancelButton" onMouseClicked="cancelButtonClicked" layoutX="155.0" layoutY="59.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
